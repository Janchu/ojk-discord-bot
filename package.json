{
  "name": "ojk-discord-bot",
  "version": "1.3.0",
  "description": "OJK Discord bot",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "build": "babel -d build src --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\",\"src/**/__mocks__\"",
    "prod": "node build/index.js",
    "test": "jest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"**/*.+(js|md)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Janchu/ojk-discord-bot.git"
  },
  "keywords": [
    "ojk",
    "discord",
    "bot"
  ],
  "author": "Janne JÃ¤ppinen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Janchu/ojk-discord-bot/issues"
  },
  "homepage": "https://github.com/Janchu/ojk-discord-bot#readme",
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "axios": "^0.22.0",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "fuse.js": "^6.4.6",
    "node-vibrant": "^3.2.1-alpha.1",
    "pino": "^7.0.0-rc.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "babel-jest": "^27.2.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.2",
    "nodemon": "^2.0.20",
    "pino-pretty": "^7.0.1",
    "prettier": "^2.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ]
  }
}
